<h1>Here are your lists! </h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
        <td><%= link_to 'See more ➡️', list %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Create new list! 🎬', new_list_path, class:"btn btn-primary" %>
